{
  "manifest_version": 2,

  "name": "Workflowy code formatter",
  "description": "Makes backtick wrapped text look like code!",
  "version": "0.1.1",

  "icons": {
    "128": "workflowyCodeFormatterIcon_128.png"
  },

  "content_scripts": [{
    "matches": ["https://workflowy.com/*"],
    "js": ["workflowyCodeFormatter.min.js"],
    "css": ["workflowyCodeFormatter.css"]
  }]
}
