{
  "manifest_version": 2,

  "name": "WorkFlowy code formatter",
  "description": "Makes backtick wrapped text look like code! {openSource}",
  "version": "0.2.2",

  "icons": {
    "128": "wcf_icon.png"
  },

  "content_scripts": [{
    "matches": ["https://workflowy.com/*"],
    "js": ["prettify.min.js", "workflowyCodeFormatter.min.js"],
    "css": ["prettify.min.css", "workflowyCodeFormatter.min.css"]
  }]
}
